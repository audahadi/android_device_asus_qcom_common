# Battery
/sys/devices/soc\.0/qpnp-linearcharger-[0-9a-f]+/power_supply/battery/capacity  u:object_r:sysfs_batteryinfo:s0

# Bluetooth
/persist/.bt_nv.bin                     u:object_r:bluetooth_data_file:s0
/system/etc/init.qcom.bt.sh             u:object_r:bluetooth_loader_exec:s0
/dev/smd3                               u:object_r:hci_attach_dev:s0

# Camera
/system/lib/libarcsoft_panorama_burstcapture.so u:object_r:apk_data_file:s0
/system/lib/libcameraap.so                      u:object_r:apk_data_file:s0
/system/lib/DataSet/ispDB/ParameterDB.db        u:object_r:camera_db_file:s0

# Factory
/factory(/.*)?                          u:object_r:fac_file:s0
/system/etc/init.qcom.factory.sh        u:object_r:qcom_factory_exec:s0

# ASDF
/asdf(/.*)?                             u:object_r:asdf_data_file:s0

# Perfd
/data/misc/perfd(/.*)?                  u:object_r:mpctl_data_file:s0

# Sensors
/dev/cm3602                             u:object_r:sensors_device:s0
/dev/lightsensor                        u:object_r:sensors_device:s0

# Tfa amp
/system/bin/tfamode                     u:object_r:tfamode_exec:s0

# Touchscreen
/sys/devices/soc.0/78b9000.i2c/i2c-5/5-0038/gesture_mode u:object_r:sysfs_touchscreen:s0

# Wi-Fi
/persist/.genmac                        u:object_r:wifi_data_file:s0
